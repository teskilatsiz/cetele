{
  "expo": {
    "name": "Ã‡etele",
    "slug": "cetele",
    "version": "1.0.1",
    "description": "Privacy-first note-taking on Nostr. Encrypted, censorship-resistant, decentralized.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cetele",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "teskilatsiz",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "monochromeImage": "./assets/images/icon-monochrome.png",
        "backgroundColor": "#000000"
      },
      "navigationBarBackgroundColor": "#000000",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-secure-store",
      "expo-local-authentication",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
